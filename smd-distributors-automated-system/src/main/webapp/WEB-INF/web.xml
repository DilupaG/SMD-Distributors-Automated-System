<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
version="3.1"> 

  <servlet>
  	<servlet-name>view</servlet-name>
  	<servlet-class>com.smd.servlet.viewDeliveryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>view</servlet-name>
  	<url-pattern>/view</url-pattern>
  </servlet-mapping>
 
  
  
  <servlet>
  	<servlet-name>add</servlet-name>
  	<servlet-class>com.smd.servlet.addDeliveryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>add</servlet-name>
  	<url-pattern>/add</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
  	<servlet-name>odr</servlet-name>
  	<servlet-class>com.smd.servlet.getOrdertoDeliveryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>odr</servlet-name>
  	<url-pattern>/odr</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
  	<servlet-name>update</servlet-name>
  	<servlet-class>com.smd.servlet.updateDeliveryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>update</servlet-name>
  	<url-pattern>/update</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
  	<servlet-name>delete</servlet-name>
  	<servlet-class>com.smd.servlet.deleteDeliveryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>delete</servlet-name>
  	<url-pattern>/delete</url-pattern>
  </servlet-mapping>
</web-app>